<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<HBox alignment="CENTER_LEFT" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.arkn37.controller.NavbarController" stylesheets="@style.css">
   <padding>
      <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
   </padding>
   <children>
      <Label text="GSA Request">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <Pane HBox.hgrow="ALWAYS" />
      <HBox alignment="CENTER_RIGHT" spacing="10.0">
         <children>
            <Button onAction="#handlePageAction" text="Home" userData="home" />
            <Button onAction="#handlePageAction" text="Search" userData="search" />
            <Button onAction="#handlePageAction" text="Saved Articles" userData="articles" />
         </children>
      </HBox>
   </children>
</HBox>
